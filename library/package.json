{
  "name": "parcel-demo-lib",
  "version": "0.0.6",
  "description": "Test of parcel for libraries",
  "license": "MIT",
  "module": "dist/lib/index.mjs",
  "types": "dist/lib/types.d.ts",
  "type": "module",
  "targets": {
    "module": {
      "source": "src/index.ts"
    },
    "types": {
      "source": "src/index.ts"
    },
    "library": {
      "source": "src/index.ts",
      "isLibrary": true,
      "distDir": "dist/lib"
    },
    "demo": {
      "source": "src/demo.html",
      "distDir": "dist/demo"
    }
  },
  "files": [
    "dist/lib"
  ],
  "scripts": {
    "dev": "parcel --target demo --no-autoinstall --dist-dir dist/demo",
    "build": "parcel build"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.3.0"
  },
  "devDependencies": {
    "@parcel/packager-ts": "2.6.2",
    "@parcel/resolver-glob": "2.6.2",
    "@parcel/transformer-inline": "2.6.2",
    "@parcel/transformer-inline-string": "2.6.2",
    "@parcel/transformer-sass": "2.6.2",
    "@parcel/transformer-typescript-types": "^2.6.2",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "parcel": "2.6.2",
    "typescript": "^4.7.4"
  },
  "alias": {
    "process": false
  }
}
