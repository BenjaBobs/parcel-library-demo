{
  "name": "parcel-demo-lib",
  "version": "1.0.0",
  "description": "Test of parcel for libraries",
  "license": "MIT",
  "module": "dist/lib/module.js",
  "types": "dist/lib/types.d.ts",
  "isLibrary": true,
  "targets": {
    "library": {
      "source": "src/index.ts",
      "distDir": "dist/lib"
    },
    "demo": {
      "source": "src/demo.html",
      "distDir": "dist/demo"
    }
  },
  "scripts": {
    "dev": "parcel --target demo --no-autoinstall --dist-dir dist/demo",
    "build": "parcel build"
  },
  "dependencies": {
    "@mdx-js/react": "1.6.22",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "6"
  },
  "devDependencies": {
    "@parcel/resolver-glob": "^2.5.0",
    "@parcel/transformer-mdx": "^2.5.0",
    "@parcel/transformer-sass": "^2.5.0",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "parcel": "^2.5.0"
  }
}
